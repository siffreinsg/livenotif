{
    "name": "LiveNotif - [Name]",
    "short_name": "[Name]",
    "description": "Envoie une notification quand [Name] est en live ou sort une vid√©o.",
    "version": "4.0.0",
    "browser_action": {
        "default_title": "[Name] est hors-ligne.",
        "default_icon": {
            "16": "assets/icons/off/16.png",
            "32": "assets/icons/off/32.png",
            "48": "assets/icons/off/48.png",
            "128": "assets/icons/off/128.png"
        }
    },
    "icons": {
        "16": "assets/icons/on/16.png",
        "32": "assets/icons/on/32.png",
        "48": "assets/icons/on/48.png",
        "128": "assets/icons/on/128.png"
    },
    "background": {
        "scripts": [
            "assets/js/browser-polyfill.min.js",
            "assets/js/timeago.min.js",
            "config.js",
            "variables.js",
            "tools.js",
            "events.js",
            "app.js"
        ]
    },
    "permissions": [
        "https://i.ytimg.com/*",
        "https://static-cdn.jtvnw.net/*",
        "https://api.siffreinsigy.me/*",
        "notifications",
        "storage"
    ],
    "author": "Siffrein Sigy",
    "manifest_version": 2
}
